{
  "name": "layout",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build-css": "npx tailwindcss build -i ./src/index.css -o ./src/output.css",
    "watch-css": "npx tailwindcss build -i ./src/index.css -o ./src/output.css --watch",
    "start": "concurrently \"npm run dev\" \"npm run watch-css\""
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "d3": "^7.9.0",
    "framer-motion": "^11.11.17",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1",
    "tailwindcss": "^3.4.15",
    "tailwindcss-filters": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
